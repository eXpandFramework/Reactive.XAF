<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ApplicationIcon>ExpressApp.ico</ApplicationIcon>
    <OutputType>WinExe</OutputType>
    <AssemblyOriginatorKeyFile>..\..\src\Xpand.key\Xpand.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <TargetFramework>net9.0-windows7.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <OutputPath>..\..\bin\</OutputPath>
    <LangVersion>latest</LangVersion>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>true</AppendTargetFrameworkToOutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <LangVersion>latest</LangVersion>
    <GenerateSerializationAssemblies>Off</GenerateSerializationAssemblies>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <LangVersion>latest</LangVersion>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System.Configuration" />
    <Reference Include="System.Windows.Forms">
      <Private>False</Private>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="build\**" />
    <Compile Remove="IDE\**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="build\**" />
    <EmbeddedResource Remove="IDE\**" />
    <None Remove="ExpressApp.ico" />
    <None Remove="paket.dependencies" />
    <None Remove="Xpand.XAF.ModelEditor.WinDesktop.csproj.paket.references" />
    <None Remove="Xpand.XAF.ModelEditor.WinDesktop.paket.references" />
    <EmbeddedResource Include="ExpressApp.ico" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="9.0.8" />
    <PackageReference Include="DevExpress.ExpressApp" Version="25.2.3" />
    <PackageReference Include="DevExpress.ExpressApp.Win" Version="25.2.3" />
    <PackageReference Include="DevExpress.ExpressApp.Xpo" Version="25.2.3" />
    <PackageReference Include="DevExpress.Persistent.Base" Version="25.2.3" />
    <PackageReference Include="DevExpress.Persistent.BaseImpl.Xpo" Version="25.2.3" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="build\**" />
    <None Remove="IDE\**" />
  </ItemGroup>
</Project>