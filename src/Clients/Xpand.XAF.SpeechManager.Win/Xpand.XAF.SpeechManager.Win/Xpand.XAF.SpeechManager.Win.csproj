<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows7.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <ApplicationIcon>ExpressApp.ico</ApplicationIcon>
    <Deterministic>false</Deterministic>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <ImplicitUsings>enable</ImplicitUsings>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\..\Xpand.key\xpand.snk</AssemblyOriginatorKeyFile>
    <LangVersion>latest</LangVersion>
    <DebugSymbols>true</DebugSymbols>
    <ProduceReferenceAssembly>false</ProduceReferenceAssembly>
    <DebugType>full</DebugType>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
    <AppendTargetFrameworkToOutputPath>True</AppendTargetFrameworkToOutputPath>
    <ToolCommandName>Speech</ToolCommandName>
    <PackageOutputPath>$(MSBuildThisFileDirectory)nupkgs</PackageOutputPath>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <OutputPath>..\..\..\..\bin\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <NoWarn>1701;1702;XAF0017</NoWarn>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <NoWarn>1701;1702;XAF0017</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <Using Include="DevExpress.ExpressApp.ListView" Alias="ListView" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Images\Logo.svg" />
    <None Remove="Model.DesignedDiffs.xafml" />
    <None Remove="Model.xafml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\Common\AssemblyInfoVersion.cs" Link="Properties\AssemblyInfoVersion.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Images\Logo.svg" />
    <Content Include="Model.xafml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="DevExpress.ExpressApp.Win.All" Version="25.1.7" />
    <PackageReference Include="DevExpress.ExpressApp.Xpo" Version="25.1.7" />
    <PackageReference Include="DevExpress.Persistent.Base" Version="25.1.7" />
    <PackageReference Include="DevExpress.Persistent.BaseImpl.Xpo" Version="25.1.7" />
    <PackageReference Include="DevExpress.RichEdit.Export" Version="25.1.7" />
    <PackageReference Include="DevExpress.Win.Design" Version="25.1.7" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Xpand.XAF.SpeechManager.Module\Xpand.XAF.SpeechManager.Module.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="xpand.xaf.Win.all" Version="4.221.11.1" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Xpand.XAF.Modules.SpellChecker">
      <HintPath>..\..\..\..\bin\net6.0-windows7.0\Xpand.XAF.Modules.SpellChecker.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>