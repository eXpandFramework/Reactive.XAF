<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <LangVersion>latest</LangVersion>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\Xpand.key\xpand.snk</AssemblyOriginatorKeyFile>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>pdbonly</DebugType>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="HideToolBar\**" />
    <Compile Remove="MasterDetail\**" />
    <EmbeddedResource Remove="HideToolBar\**" />
    <EmbeddedResource Remove="MasterDetail\**" />
    <None Remove="HideToolBar\**" />
    <None Remove="MasterDetail\**" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Fasterflect.Netstandard">
      <HintPath>..\..\Libs\Fasterflect.Netstandard.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Moq" Version="4.12.0" />
    <PackageReference Include="Shouldly" Version="3.0.2" />
    <PackageReference Include="System.Reactive" Version="4.1.5" />
    <PackageReference Include="xunit" Version="2.4.1" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="DevExpress.ExpressApp" Version="18.2.7" />
    <PackageReference Include="DevExpress.ExpressApp.Win" Version="18.2.7" />
    <PackageReference Include="DevExpress.Win" Version="18.2.7" />
    <PackageReference Include="DevExpress.Win" Version="18.2.7" />
    <PackageReference Include="DevExpress.Data" Version="18.2.7" />
    <PackageReference Include="DevExpress.Xpo" Version="18.2.7" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Extensions\Extensions\Xpand.Source.Extensions.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Tests">
      <HintPath>..\..\..\bin\TestsLib.dll</HintPath>
    </Reference>
    <Reference Include="Xpand.XAF.Modules.HideToolBar">
      <HintPath>..\..\..\bin\Xpand.XAF.Modules.HideToolBar.dll</HintPath>
    </Reference>
    <Reference Include="Xpand.XAF.Modules.Reactive">
      <HintPath>..\..\..\bin\Xpand.XAF.Modules.Reactive.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>