    <Reference Include="Xpand.XAF.Modules.HideToolbar">
      <HintPath>..\..\..\bin\Xpand.XAF.Modules.HideToolbar.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
