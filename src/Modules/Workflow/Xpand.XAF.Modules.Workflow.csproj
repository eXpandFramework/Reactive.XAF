      <HintPath>..\..\..\bin\Xpand.Extensions.Reactive.Relay.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Xpand.XAF.Modules.BulkObjectUpdate">
      <HintPath>..\..\..\bin\Xpand.XAF.Modules.BulkObjectUpdate.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
