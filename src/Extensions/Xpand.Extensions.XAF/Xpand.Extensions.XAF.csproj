<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <LangVersion>latest</LangVersion>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\Xpand.key\xpand.snk</AssemblyOriginatorKeyFile>
    <DebugSymbols>true</DebugSymbols>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateBindingRedirectsOutputType>true</GenerateBindingRedirectsOutputType>
    <OutputPath>..\..\..\bin</OutputPath>
    <AssemblyTitle>Xpand.Extensions.XAF</AssemblyTitle>
    <Product>Xpand.Extensions.XAF</Product>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DevExpress.ExpressApp" Version="18.2.7" />
    <PackageReference Include="Fasterflect.Xpand" Version="2.0.6" />
    <PackageReference Include="System.Reflection.Emit" Version="4.6.0" />
    <!-- <PackageReference Include="System.Reflection.Emit.Lightweight" Version="4.6.0" /> -->
    <PackageReference Include="Xpand.Patcher" Version="1.0.1" />
  </ItemGroup>
</Project>